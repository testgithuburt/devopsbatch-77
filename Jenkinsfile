pipeline {
  agent any
  stages {
    stage('dev') {
      steps {
        echo 'this is dev stage'
      }
    }

    stage('test') {
      steps {
        echo 'this is test stage'
      }
    }

    stage('build') {
      steps {
        echo 'this is build stage'
      }
    }

    stage('deploy') {
      steps {
        echo 'deploy stage'
      }
    }

    stage('production') {
      steps {
        echo 'production '
      }
    }

  }
}